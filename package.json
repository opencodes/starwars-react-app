  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "test:single": "npm run test -- src/App.test.tsx ",
    "test:coverage": "npm run test -- --coverage --u ",
    "test:coverageAll": "npm run test -- --coverage --watchAll"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
